#include<iostream>
using namespace std;
int main()
{
    int n,i,m;;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        if((a[i]==1) || (a[i]==4) || (a[i]==9) || (a[i]==16) || (a[i]==25))
        {
            m=m+a[i];
        }
    }cout<<m;
}
