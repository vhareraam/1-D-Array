#include<iostream>
using namespace std;
int main()
{
    int m,i,n,j,c,d;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        if(a[i]%2==0)
        c++;
        else
        d++;
    }
    if(c==n)
    cout<<"Even";
    else if(d==n)
    cout<<"Odd";
    else
    cout<<"Mixed";
}
