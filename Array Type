#include<iostream>
using namespace std;
int main(){
    int n,i,m,o,q;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        
        if(a[i]%2==0)
        {
            m++;
        }
        else if(a[i]%2==1){
            q++;
        }
        else{
            o++;
        }
    }
    //cout<<m<<endl<<n<<endl<<o;
        if(q==0 && o==0){
            cout<<"Even";
        }
        else if(m==0 && o==0){
            cout<<"Odd";
        }
        else {
            cout<<"Mixed";
        }
    
}
