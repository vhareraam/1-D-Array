#include<iostream>
using namespace std;
int main(){
    int n,i,j,m,o;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        
        if(a[i]%2==0)
        {
            m++;
            
        }
        else {
            o++;
            
        }
        
    }cout<<"Odd: "<<o<<endl;
     cout<<"Even: "<<m<<endl;
     
}
