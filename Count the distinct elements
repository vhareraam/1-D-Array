#include<iostream>
using namespace std;
int main()
{
    int n,i,j,k=1;
    cin>>n;
    int a[n];
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for (i = 1; i < n; i++)
    {
      for (j = 0; j < i; j++)
      {
         if (a[i] == a[j]){
            break;
         }
      }
      if (i == j){
         k++;
      }
    } 
      cout<<k;
    
}
